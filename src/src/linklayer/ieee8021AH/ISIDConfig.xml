<?xml version="1.0"?>
<!-- I-SID static configuration example. -->
<!-- Registering ISid 1 to 4 at ports 0 to 6 -->
<isidConfig>
	<Port>
		<index>0</index>
		<ISids>
			<ISid>1</ISid>
			<ISid>2</ISid>
			<ISid>3</ISid>
			<ISid>4</ISid>
		</ISids>
	</Port>
	<Port>
		<index>1</index>
		<ISids>
			<ISid>1</ISid>
			<ISid>2</ISid>
			<ISid>3</ISid>
			<ISid>4</ISid>
		</ISids>
	</Port>
	<Port>
		<index>2</index>
		<ISids>
			<ISid>1</ISid>
			<ISid>2</ISid>
			<ISid>3</ISid>
			<ISid>4</ISid>
		</ISids>
	</Port>
	<Port>
		<index>3</index>
		<ISids>
			<ISid>1</ISid>
			<ISid>2</ISid>
			<ISid>3</ISid>
			<ISid>4</ISid>
		</ISids>
	</Port>
	<Port>
		<index>4</index>
		<ISids>
			<ISid>1</ISid>
			<ISid>2</ISid>
			<ISid>3</ISid>
			<ISid>4</ISid>
		</ISids>
	</Port>
	<Port>
		<index>5</index>
		<ISids>
			<ISid>1</ISid>
			<ISid>2</ISid>
			<ISid>3</ISid>
			<ISid>4</ISid>
		</ISids>
	</Port>
	<Port>
		<index>6</index>
		<ISids>
			<ISid>1</ISid>
			<ISid>2</ISid>
			<ISid>3</ISid>
			<ISid>4</ISid>
		</ISids>
	</Port>
</isidConfig>